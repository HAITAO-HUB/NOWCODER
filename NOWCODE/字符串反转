


import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner scan=new Scanner(System.in);
        String s=scan.nextLine();
        String a="";
        for(int i=s.length()-1;i>=0;i--){
            a=a+s.charAt(i);
        }
        System.out.println(a);
    }
}